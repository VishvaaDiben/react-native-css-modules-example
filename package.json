{
  "name": "CSSModulesExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "stylelint 'src/**/*.@(css|scss)'",
    "web": "webpack-dev-server --mode development",
    "ios": "react-native run-ios",
    "android": "react-native run-android"
  },
  "dependencies": {
    "react": "^16.3.0-alpha.1",
    "react-dom": "^16.2.0",
    "react-fontawesome": "^1.6.1",
    "react-native": "0.54.4",
    "react-native-fontawesome": "^5.7.0",
    "react-native-web": "^0.5.3"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-native-classname-to-style": "^1.2.1",
    "babel-plugin-react-native-platform-specific-extensions": "^1.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "4.0.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "jest": "22.4.3",
    "node-sass": "^4.8.3",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.3",
    "react-hot-loader": "^4.0.0",
    "react-native-css-transformer": "^1.1.0",
    "react-native-sass-transformer": "^1.1.1",
    "react-test-renderer": "16.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-react-native-css-modules": "^1.4.0",
    "stylelint-react-native": "^1.1.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11",
    "Android >= 4.4",
    "iOS >= 8"
  ],
  "jest": {
    "preset": "react-native"
  },
  "stylelint": {
    "extends": "stylelint-config-react-native-css-modules",
    "rules": {
      "selector-class-pattern": "^[a-z][a-zA-Z0-9]*$",
      "declaration-block-no-duplicate-properties": true,
      "no-duplicate-selectors": true,
      "no-extra-semicolons": true,
      "unit-whitelist": [
        "px",
        "rem",
        "%"
      ],
      "no-eol-whitespace": true,
      "no-missing-end-of-source-newline": true
    }
  }
}
