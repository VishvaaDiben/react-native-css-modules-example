{
  "name": "CSSModulesExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint . && stylelint 'src/**/*.@(css|scss)'",
    "web": "webpack-dev-server --mode development",
    "ios": "react-native run-ios",
    "android": "react-native run-android"
  },
  "dependencies": {
    "es6-shim": "^0.35.5",
    "react": "16.6.3",
    "react-art": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-fontawesome": "^1.6.1",
    "react-native": "0.58.6",
    "react-native-fontawesome": "^5.7.0",
    "react-native-web": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.0",
    "autoprefixer": "^9.5.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-native-classname-to-style": "^1.2.2",
    "babel-plugin-react-native-platform-specific-extensions": "^1.1.1",
    "babel-preset-react": "^7.0.0-beta.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-plugin-css-modules": "^2.11.0",
    "file-loader": "^3.0.1",
    "jest": "24.5.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.14",
    "postcss-css-variables": "^0.12.0",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "3.1.3",
    "react-native-postcss-transformer": "^1.2.4",
    "react-native-sass-transformer": "^1.4.0",
    "react-test-renderer": "16.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-react-native-css-modules": "^2.2.0",
    "stylelint-react-native": "^2.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11",
    "Android >= 4.4",
    "iOS >= 8"
  ],
  "jest": {
    "preset": "react-native"
  },
  "postcss": {
    "plugins": {
      "postcss-css-variables": {}
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "css-modules"
    ],
    "extends": [
      "plugin:css-modules/recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-react-native-css-modules",
    "rules": {
      "selector-class-pattern": "^[a-z][a-zA-Z0-9]*$",
      "declaration-block-no-duplicate-properties": true,
      "no-duplicate-selectors": true,
      "no-extra-semicolons": true,
      "no-eol-whitespace": true,
      "no-missing-end-of-source-newline": true
    }
  }
}
